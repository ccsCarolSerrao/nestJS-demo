    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.3",
    "@types/faker": "^4.1.10",
    "@types/helmet": "^0.0.45",
    "@types/jest": "25.1.4",
    "@types/newrelic": "^6.4.0",
    "@types/node": "^13.9.1",
