  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack --config .webpack/webpack.config.js",
    "build:nest": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"__tests__/**/*.ts\" \"__mocks__/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,__test__,__mocks__}/**/*.ts\"",
    "lint:fix": "eslint \"{src,__test__,__mocks__}/**/*.ts\" --fix",
    "test": "jest --config ./__tests__/units/jest.config.js",
    "test:watch": "jest --config ./__tests__/units/jest.config.js --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./__tests__/functionals/jest.config.js"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
